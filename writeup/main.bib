journal = {Sensors and Actuators, B: Chemical},
keywords = {Artificial neural networks,Automatic Bayesian regularization,Electronic nose,Electronic nose design,Feature selection,Multisensor device,On-field calibration,Urban air pollution monitoring},
mendeley-groups = {Air Quality Papers,Air Quality Papers/Electronic Noses/Arrays,Air Quality Papers/Sensor Quant. {\&} Monitoring I,MetaSense Ref},
number = {2},
pages = {182--191},
title = {{CO, NO2, and NOx urban pollution monitoring with on-field calibrated electronic nose by automatic bayesian regularization}},
