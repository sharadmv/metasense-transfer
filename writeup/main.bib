@article{Spinelle2015,
abstract = {Abstract The performances of several field calibration methods for low-cost sensors, including linear/multi linear regression and supervised learning techniques are compared. A cluster of ozone, nitrogen dioxide, nitrogen monoxide, carbon monoxide and carbon dioxide sensors was operated. The sensors were either of metal oxide or electrochemical type or based on miniaturized infra-red cell. For each method, a two-week calibration was carried out at a semi-rural site against reference measurements. Subsequently, the accuracy of the predicted values was evaluated for about five months using a few indicators and techniques: orthogonal regression, target diagram, measurement uncertainty and drifts over time of sensor predictions. The study assessed if the sensors were could reach the Data Quality Objective (DQOs) of the European Air Quality Directive for indicative methods (between 25 and 30{\%} of uncertainty for O{\textless}inf{\textgreater}3{\textless}/inf{\textgreater} and NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater}). In this study it appears that O{\textless}inf{\textgreater}3{\textless}/inf{\textgreater} may be calibrated using simple regression techniques while for NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} a better agreement between sensors and reference measurements was reached using supervised learning techniques. The hourly O{\textless}inf{\textgreater}3{\textless}/inf{\textgreater} DQO was met while it was unlikely that NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} hourly one could be met. This was likely caused by the low NO{\textless}inf{\textgreater}2{\textless}/inf{\textgreater} levels correlated with high O{\textless}inf{\textgreater}3{\textless}/inf{\textgreater} levels that are typical of semi-rural site where the measurements of this study took place.},
annote = {sensor background},
author = {Spinelle, Laurent and Gerboles, Michel and Villani, Maria Gabriella and Aleixandre, Manuel and Bonavitacola, Fausto},
doi = {10.1016/j.snb.2015.03.031},
file = {:C$\backslash$:/Users/ashleymonika/Dropbox/Documents/Papers/5-18-16/1-s2.0-S092540051500355X-main.pdf:pdf},
isbn = {0925-4005},
issn = {09254005},
journal = {Sensors and Actuators, B: Chemical},
keywords = {Air Quality Directive,Gas sensors,Measurement uncertainty,Multivariate linear regression,Neural network,Validation},
mendeley-groups = {Air Quality Papers,Air Quality Papers/Sensor Quant. {\&} Monitoring I,MetaSense Ref},
pages = {249--257},
publisher = {Elsevier B.V.},
title = {{Field calibration of a cluster of low-cost available sensors for air quality monitoring. Part A: Ozone and nitrogen dioxide}},
url = {http://dx.doi.org/10.1016/j.snb.2015.03.031},
volume = {215},
year = {2015}
}

